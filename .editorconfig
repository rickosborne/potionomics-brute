[*.js]
tab_width = 2
indent_size = tab
indent_style = tab
insert_final_newline = true
ij_javascript_force_semicolon_style = true
ij_javascript_use_semicolon_after_statement = true
ij_javascript_enforce_trailing_comma = whenmultiline

[{*.tsv,*.csv}]
tab_width = 4
indent_size = tab
indent_style = tab
insert_final_newline = true
trim_trailing_whitespace = false
